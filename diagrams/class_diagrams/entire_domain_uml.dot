@startuml

title "Domain Class Diagram"

class AuthenticationToken
class Authenticator
class BranchOffice
class Company
class DelegatedStatus
class Delegation
class DelegationOffice
class Developer
class ExecutingStatus
class FailedStatus
class FinishedStatus
class Manager
class PlannedStatus
class Planning
class Project 
class Requirement {
    int: amount
}
class Requirements
class Resource
class Resource 
class ResourceType
class Task 
class TaskInfo
class TaskMan
class TaskStatus
class TimePeriod
class User
interface Reservable

Reservable <|-- Developer
Reservable <|-- Resource
TaskStatus <|-- DelegatedStatus
TaskStatus <|-- ExecutingStatus
TaskStatus <|-- FailedStatus
TaskStatus <|-- FinishedStatus
TaskStatus <|-- PlannedStatus
User <|-- Developer
User <|-- Manager

AuthenticationToken --> "1" BranchOffice
AuthenticationToken --> "1" User
BranchOffice --> "*" Project
BranchOffice --> "*" Resource
BranchOffice --> "*" User
Company --> "*" BranchOffice
Company --> "*" ResourceType
Company --> "1" DelegationOffice
DelegatedStatus --> "1" Delegation
Delegation --> "1" BranchOffice: "from"
Delegation --> "1" BranchOffice: "to"
Delegation --> "1" Task: "delegatedTask"
DelegationOffice --> "*" Delegation
ExecutingStatus --> "1" Planning
FailedStatus --> "0..1" Task: "alternative"
FailedStatus --> "1" TimePeriod
FinishedStatus --> "1" TimePeriod
PlannedStatus --> "1" Planning
Project --> "*" Task
Requirement --> "*" Requireable
Requirements --> "*" Requirement
Resource --> "1" ResourceType
Task --> "1" TaskInfo
Task --> "1" TaskStatus
TaskInfo --> "*" Requirements
TaskInfo --> "*" TaskInfo: "dependsOn"
TaskMan --> "1" Authenticator
TaskMan --> "1" Company

@enduml
